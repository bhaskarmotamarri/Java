
import java.io.*;
import java.util.*;
class ThreadCreation2 implements Runnable{
    public void run(){
        System.out.println("Thread was created and it is running");
    }
    public static void main(String args[])
    {
        ThreadCreation2 tc2=new ThreadCreation2();
        Thread t=new Thread(tc2);
        t.start();
    }
}
